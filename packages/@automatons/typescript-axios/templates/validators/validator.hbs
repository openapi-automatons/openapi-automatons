import * as Yup from "yup";
{{#each imports~}}
    import { {{title}}Validator } from "./{{filename}}";
{{/each}}
{{#eq schema/type 'object'}}
import { {{title}} } from "../models";
{{else eq schema/type 'allOf'}}
import { {{title}} } from "../models";
{{else eq schema/type 'oneOf'}}
import { {{title}} } from "../models";
{{else eq schema/type 'array'}}
import { {{title}} } from "../models";
{{/eq}}

export const {{title}}Validator = {{> validators/type schema title=title}};
