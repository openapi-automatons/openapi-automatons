private static server(server: {{#each servers}}{{name}}Server{{#unless @last}} | {{/unless}}{{/each}}) {
	{{#each servers}}
	if ('{{name}}' === server.name) {
		return '{{url}}'
		{{#each values}}
			.replace('{ {{~name~}} }', template('{{name}}', server.values.{{name}}, 'simple', false))
		{{~/each}};
	}
	{{/each}}
	throw new Error('Undefined server. please define server.');
}
