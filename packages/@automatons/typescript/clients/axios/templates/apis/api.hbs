import { AbstractApi, AbstractConfig } from "./abstractApi";
import { Config, Security } from "../config";
{{#with meta}}
	{{#or hasTemplate hasQuery ~}}
		import { {{#if hasTemplate}}template{{#if hasQuery}},{{/if}}{{/if}} {{#if hasQuery}}query{{/if}}  } from "../utils";
	{{/or}}
{{/with}}
